{
  "name": "Where is my ",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage", 
    "activeTab", 
    "scripting", 
    "input"
  ],
  "host_permissions": [
    "https://www.googleapis.com/customsearch/v1/*"
  ],
  "commands": {
    "run": {
      "suggested_key": "Ctrl+Shift+Y",
      "description": "Fix the highlighted text."
    }
  },
  "action": {
    "default_popup": "main.html"
  }
}